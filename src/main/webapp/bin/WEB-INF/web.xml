<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
         http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <display-name>Projet_Calbo</display-name>

    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>EquipeServlet</servlet-name>
        <servlet-class>Projet_Calbo.controller.EquipeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EquipeServlet</servlet-name>
        <url-pattern>/equipe</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>TacheServlet</servlet-name>
        <servlet-class>Projet_Calbo.controller.TacheServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TacheServlet</servlet-name>
        <url-pattern>/tache</url-pattern>
    </servlet-mapping>

</web-app>